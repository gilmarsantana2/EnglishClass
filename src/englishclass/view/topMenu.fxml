<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane fx:id="topPane" minWidth="600.0" prefHeight="35.0" prefWidth="600.0" stylesheets="@css/topmenu.css"
            xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <Pane layoutX="407.0" layoutY="6.0" prefHeight="25.0" prefWidth="25.0" styleClass="menu"
         AnchorPane.rightAnchor="163.0" AnchorPane.topAnchor="6.0">
      <children>
         <ImageView fx:id="notificationIcon" fitHeight="25.0" fitWidth="25.0" onMouseClicked="#showNotification"
                    pickOnBounds="true" preserveRatio="true">
            <image>
               <Image url="@../../../res/icons/bell.png"/>
            </image>
         </ImageView>
      </children>
   </Pane>
   <Pane layoutX="469.0" layoutY="6.0" prefHeight="25.0" prefWidth="25.0" styleClass="menu"
         AnchorPane.rightAnchor="101.0" AnchorPane.topAnchor="6.0">
      <children>
         <ImageView fx:id="settingIcon" fitHeight="25.0" fitWidth="25.0" onMouseClicked="#showSettings"
                    pickOnBounds="true" preserveRatio="true">
            <image>
               <Image url="@../../../res/icons/settings.png"/>
            </image>
         </ImageView>
      </children>
   </Pane>
   <Pane layoutX="536.0" layoutY="7.0" prefHeight="25.0" prefWidth="25.0" styleClass="menu"
         AnchorPane.rightAnchor="34.0" AnchorPane.topAnchor="6.0">
      <children>
         <ImageView fx:id="logoutIcon" fitHeight="25.0" fitWidth="25.0" onMouseClicked="#logout" pickOnBounds="true"
                    preserveRatio="true" styleClass="menu">
            <image>
               <Image url="@../../../res/icons/opened-door.png"/>
            </image>
         </ImageView>
      </children>
   </Pane>
   <Pane layoutX="23.0" layoutY="6.0" prefHeight="25.0" prefWidth="25.0" styleClass="menu" AnchorPane.bottomAnchor="6.0"
         AnchorPane.leftAnchor="23.0" AnchorPane.topAnchor="6.0">
      <children>
         <ImageView fx:id="menuIcon" fitHeight="25.0" fitWidth="25.0" layoutX="1.0" layoutY="-1.0"
                    onMouseClicked="#showMenu" pickOnBounds="true" preserveRatio="true">
            <image>
               <Image url="@../../../res/icons/menu.png"/>
            </image>
         </ImageView>
      </children>
   </Pane>
   <Label fx:id="userName" alignment="CENTER" contentDisplay="CENTER" layoutX="227.0" layoutY="10.0" text="User Name"
          AnchorPane.topAnchor="10.0"/>
</AnchorPane>
